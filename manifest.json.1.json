{
  "name": "Sample",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Sample Chrome Extension",
  "permissions": [
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "matches": ["https://*/*"],
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [{
    "matches": ["https://pages.awscloud.com/*"],
    "js": [
      "js/content.js",
      "js/jquery.min.js"
    ]
  }],
  "browser_action": {
    "default_title": "Auto Webinar",
    "default_popup": "popup/popup.html"
  }
}
